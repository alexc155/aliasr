{
  "name": "aliasr",
  "version": "1.0.0",
  "description": "Create command line aliases",
  "main": "./bin/aliasr.js",
  "bin": {
    "aliasr": "bin/aliasr.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexc155/aliasr.git"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "test": "mocha $(find . -name '*.spec.js' ! -ipath '*node_modules*') --reporter spec",
    "test:watch": "npm install && mocha $(find . -name '*.spec.js' ! -ipath '*node_modules*') --reporter min --watch",
    "cover": "nyc --reporter=lcov --reporter=text npm run test"
  },
  "keywords": [
    "cli",
    "shortcut",
    "utility",
    "alias",
    "command"
  ],
  "author": "Alex Clark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexc155/aliasr/issues"
  },
  "homepage": "https://github.com/alexc155/aliasr",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0"
  },
  "dependencies": {
    "update-notifier": "^4.1.0"
  },
  "readme": "./readme.md"
}
